<h1>Chefs on duty</h1>
<ul>
  {{#each chefs as |chef|}}
    {{#if chef.isAvailable}}
      <li>
        {{chef.name}}
        <button {{action "makeUnavailable" chef}}>Make Unavailable</button>
      </li>
    {{else}}
      <li>
        <s>{{chef.name}}</s>
        <button {{action "makeAvailable" chef}}>Make Available</button>
      </li>
    {{/if}}
  {{/each}}
</ul>



















